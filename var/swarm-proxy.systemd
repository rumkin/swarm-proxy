[Unit]
Description=Swarm Proxy Service
After=network.target

[Service]
User=swarm-proxy
Group=swarm-proxy
WorkingDirectory=/var/www/swarm-proxy
Type=forking
PIDFile=/var/www/swarm-proxy/proxy.pid

ExecStart=/usr/bin/node cli.js start
ExecStop=/usr/bin/node cli.js stop
ExecReload=/usr/bin/node cli.js restart

[Install]
WantedBy=multi-user.target
